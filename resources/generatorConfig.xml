<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration 
    PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
    "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="memberlist"  targetRuntime="MyBatis3">
        <property name="autoDelimitKeyWords" value="true" />
        <property name="beginningDelimiter" value="`" />
        <property name="endingDelimiter" value="`" />
        <jdbcConnection
            connectionURL="jdbc:mysql://localhost/memberlist?serverTimezone=JST"
            driverClass="com.mysql.cj.jdbc.Driver"
            password=""
            userId="root" />
        <javaModelGenerator
            targetPackage="org.ranc.memberlistapp.domain.model"
            targetProject="memberlist-app/src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <sqlMapGenerator
            targetPackage="org.ranc.memberlistapp.domain.repository"
            targetProject="memberlist-app/src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
        <javaClientGenerator
            targetPackage="org.ranc.memberlistapp.domain.repository"
            targetProject="memberlist-app/src/main/java" type="XMLMAPPER">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>
        <table tableName="phone_book" domainObjectName="PhoneBookModel"
            mapperName="PhoneBookRepository"
            enableInsert="true"
            enableSelectByPrimaryKey="true"
            enableSelectByExample="true"
            enableUpdateByPrimaryKey="true"
            enableUpdateByExample="true"
            enableDeleteByPrimaryKey="true"
            enableDeleteByExample="true"
            enableCountByExample="true"
            selectByExampleQueryId="true"
            modelType="flat" >
            <columnRenamingRule searchString="^class$" replaceString="clazz"/>
            <columnOverride column="id" delimitedColumnName="true"/>
            <columnOverride column="name" delimitedColumnName="true"/>
        </table>
    </context>
</generatorConfiguration>